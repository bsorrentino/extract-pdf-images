{
  "name": "@bsorrentino/pdf-tools",
  "version": "1.1.0",
  "description": "",
  "main": "index.js",
  "bin": {
    "pdftools": "./cli.js"
  },
  "files": [
    "bin"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "clean": "./zx-clean.mjs",
    "changelog": "git-changelog-command-line -of CHANGELOG.md"
  },
  "author": "bsorrentino <bartolomeo.sorrentino@gmail.com> (http://soulsoftware-bsc.blogspot.it/)",
  "license": "MIT",
  "dependencies": {
    "canvas": "^2.9.1",
    "commander": "^9.1.0",
    "enumify": "^2.0.0",
    "jimp": "^0.16.1",
    "pdfjs-dist": "^2.13.216"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "^16.11.26",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.3",
    "typescript": "^4.6.2",
    "zx": "^6.0.6"
  },
  "engines": {
    "node": ">=16"
  },
  "standard-version": {
    "skip": {
      "tag": true
    },
    "types": [
      {
        "type": "feat",
        "section": "Features"
      },
      {
        "type": "fix",
        "section": "Bug Fixes"
      },
      {
        "type": "chore",
        "hidden": true
      },
      {
        "type": "docs",
        "section": "Documentation"
      },
      {
        "type": "style",
        "hidden": true
      },
      {
        "type": "refactor",
        "section": "Refactoring"
      },
      {
        "type": "perf",
        "hidden": true
      },
      {
        "type": "test",
        "hidden": true
      },
      {
        "type": "build",
        "section": "Build"
      }
    ],
    "commitUrlFormat": "https://github.com/mokkapps/changelog-generator-demo/commits/{{hash}}",
    "compareUrlFormat": "https://github.com/mokkapps/changelog-generator-demo/compare/{{previousTag}}...{{currentTag}}"
  }
}
